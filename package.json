    "pug": "^2.0.3",
    "sass-loader": "^7.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "nodemon": "^1.18.4",
    "pre-commit": "^1.2.2"
  },
  "pre-commit": [
    "lint"
  ]
